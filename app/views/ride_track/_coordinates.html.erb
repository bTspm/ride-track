<%= form_tag(ride_track_estimate_path, method: :get, remote: true) do %>
  <div class="form-group">
    <label>Origin</label>
    <input type="text" value="<%= params[:origin_address] %>" name="origin_address" class="form-control"  id="origin_address" placeholder="Origin"/>
  </div>
  <div class="form-group">
    <label>Destination</label>
    <input type="text" value="<%= params[:destination_address] %>" name="destination_address" class="form-control" id="destination_address" placeholder="Destination"/>
  </div>

  <input type="button" value="Submit" class="estimate btn btn-primary" >
<% end %>

<%= render partial: 'coordinate_details' %>